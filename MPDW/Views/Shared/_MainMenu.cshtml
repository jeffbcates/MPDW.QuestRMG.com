@model Quest.MPDW.Models.Menus.MainMenu

<div id="_questMainMenu" class="qrmgctx">
    <input type="hidden" id="_mainMenu" name="_mainMenu" value="@Html.Raw(Json.Encode(Model))" />
</div>



<script type="text/javascript">
    var _questMainMenu;
    $(function () {
        var _oo = $.parseJSON($('#_mainMenu').val());
        var _mm = {
            element: 'mainMenuFrame',
            prefix: '_mm',
            float: 'right',
            ctx: {
                element: '_questmm',
                elements: ['_questctx', '_questnb']
            },
            options: _oo.Options,
        }
        _questMainMenu = new qrmgMainMenu(_mm);
    });
</script>